  <define name="journal-id">
    <element name="journal-id">
      <ref name="journal-id-attlist"/>
      <text/>
    </element>
  </define>
  <!--
    journal-id-type   
      Indicates whose identifier this is, for
      example, "pub-id" for a publisher's
