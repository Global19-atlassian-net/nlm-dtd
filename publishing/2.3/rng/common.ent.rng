  <define name="anonymous">
    <element name="anonymous">
      <ref name="anonymous-attlist"/>
      <ref name="anonymous-model"/>
    </element>
  </define>
  <define name="anonymous-attlist" combine="interleave">
    <empty/>
  </define>
